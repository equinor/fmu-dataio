          "default": null,
          "title": "Giip"
        },
        "HCPV": {
          "anyOf": [
            {
              "minimum": 0.0,
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Hcpv"
        },
        "LICENSE": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "License"
        },
        "NET": {
          "minimum": 0.0,
          "title": "Net",
          "type": "number"
